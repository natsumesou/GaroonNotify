{
  "name": "GaroonNotify",
  "version": "0.5",
  "description": "Garoon v3 schedule notification extension",
  "icons": {
      "48": "images/garoon_48.png",
      "128": "images/garoon_128.png"
  },
  "browser_action": {
      "default_icon": "images/garoon_48.png",
      "default_popup": "views/popup.html"
  },
  "background_page": "background.html",
  "permissions": [
      "notifications",
      "https://*/*",
      "http://*/*"
  ]
}
